<mvc:View controllerName="pt.pcdinis.ui5webcenter.webcenter.controller.MainView" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:u="sap.ui.unified">
	<u:Shell id="idGlobalShell">
		<u:headEndItems>
			<u:ShellHeadItem icon="sap-icon://log" tooltip="Logoff" press="handleLogOffPress"></u:ShellHeadItem>
		</u:headEndItems>
		<u:content>
			<GenericTile id="tile1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Application #1" subheader="User App"
				press="pressTile1">
				<TileContent unit="EUR" footer="Current Quarter">
					<ImageContent src="https://sapui5.hana.ondemand.com/resources/sap/ui/documentation/sdk/images/logo_ui5.png"/>
				</TileContent>
			</GenericTile>
			<GenericTile id="idTileZOrangeGetLeave" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Team Leaves" subheader="Check Team Holidays"
				press="pressTileZOrangeGetLeave">
				<TileContent>
					<ImageContent src="sap-icon://check-availability"/>
				</TileContent>
			</GenericTile>
		</u:content>
	</u:Shell>
</mvc:View>
