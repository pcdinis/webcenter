<mvc:View controllerName="pt.pcdinis.ui5webcenter.webcenter.controller.MainView" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:u="sap.ui.unified">
	<u:Shell id="idGlobalShell">
		<u:headEndItems>
			<u:ShellHeadItem icon="sap-icon://log" tooltip="Logoff" press="handleLogOffPress"></u:ShellHeadItem>
		</u:headEndItems>
		<u:content>
			<GenericTile id="idTileMemGame" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Memory Game" subheader="Game"
				press="pressTileMemGame">
				<TileContent unit="" footer="">
					<ImageContent src="sap-icon://grid"/>
				</TileContent>
			</GenericTile>
			<GenericTile id="idTileMastermind" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Mastermind" subheader="Game"
				press="pressTileMastermind">
				<TileContent>
					<ImageContent src="sap-icon://legend"/>
				</TileContent>
			</GenericTile>
		</u:content>
	</u:Shell>
</mvc:View>
